//公用样式
*
  margin 0
  padding 0
  -webkit-box-sizing border-box
  -moz-box-sizing border-box
  box-sizing border-box
html, body {
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  -webkit-tap-highlight-color: rgba(0,0,0,0);
  padding: 0;
  margin: 0;
  border: none;
  background: none;
  background-color: #fafafa;
  font: 14px/24px "miui","Oxygen", Arial, "Hiragino Sans GB", "Microsoft YaHei", "微软雅黑", "STHeiti", "WenQuanYi Micro Hei", SimSun, sans-serif;
}
html,
em,
h1, h2, h3, h4, h5, h6, p, div,
body {
  margin: 0;
  padding: 0;
  font-weight: normal;
}
html,
body,
button,
input,
select,
textarea,
a {
  color: #222;
}
img,
input {
  vertical-align: middle;
}
img,
a,
fieldset {
  border: none;
}
a,
a:hover {
  text-decoration: none;
  outline: none;
}
button:focus {
  outline: none;
}
ul,
li {
  list-style: none;
  padding: 0;
  margin: 0;
}
//搜索品牌提示框
.suggest
  ul.sug-ul
    z-index 999
    background #fff
    width 50%
    position absolute
    margin 0 auto
    left 0
    right 0
    max-height 500px
    overflow auto
